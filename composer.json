{
    "name": "artemeon/console",
    "description": "Wrapper library around `symfony/console` providing a customized console styling and some helpers.",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "phpstan": "php ./vendor/bin/phpstan analyse --memory-limit=4G"
    },
    "autoload": {
        "psr-4": {
            "Artemeon\\Console\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marc Reichel",
            "email": "marc.reichel@artemeon.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.40.0",
        "laravel/prompts": "^0.3.3",
        "nunomaduro/termwind": "^1.5|^2.0",
        "symfony/console": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.2",
        "rector/rector": "^2.0.7"
    }
}
